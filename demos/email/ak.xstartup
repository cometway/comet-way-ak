<?xml version="1.0"?>
<props_list>
	<props>
		<agent_id>100</agent_id>
		<classname>com.cometway.ak.ServiceManager</classname>
		<hide_debug>true</hide_debug>
		<name>ServiceManager</name>
		<startup>true</startup>
	</props>
	<props>
		<agent_id>101</agent_id>
		<classname>com.cometway.ak.Scheduler</classname>
		<name>Scheduler</name>
		<service_name>scheduler</service_name>
		<startup>true</startup>
	</props>
	<props>
		<agent_id>200</agent_id>
		<bind_port>25</bind_port>
