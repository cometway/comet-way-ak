		<password>agent</password>
		<pop3_host>mail.domain.com</pop3_host>
		<pop3_port>110</pop3_port>
		<schedule>between 0:0:0-23:59:59 every 60s</schedule>
		<startup>true</startup>
		<username>agent</username>
